{
  "compilerOptions": {
    "module": "esnext",
    "outDir": "dist/pipedream",
    "target": "esnext",
    "lib": ["esnext"],
    "removeComments": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "node_modules/pipedream",
    "declaration": true,
    "esModuleInterop": true,
    "inlineSourceMap": false,
    "listEmittedFiles": false,
    "listFiles": false,
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "pretty": true,
    "resolveJsonModule": true,
    "rootDir": "node_modules/pipedream",
    "skipLibCheck": true,
    "strict": false,
    "traceResolution": false,
    "types": ["node"]
  },
  "compileOnSave": false,
  "include": ["node_modules/pipedream/components/**/*.mjs"]
}
